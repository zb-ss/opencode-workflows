{
  "name": "turbo",
  "description": "Speed-first mode, no high-tier models, minimizes iterations",
  "agent_routing": {
    "planning": "architect-lite",
    "implementation": "executor-lite",
    "code_review": "reviewer-lite",
    "security_review": "security-lite",
    "tests": "test-writer",
    "quality_gate": "quality-gate",
    "completion_guard": "completion-guard",
    "performance": "perf-lite",
    "codebase_analysis": "codebase-analyzer",
    "documentation": "doc-writer",
    "e2e_exploration": "e2e-explorer",
    "e2e_generation": "e2e-generator",
    "e2e_validation": "e2e-reviewer"
  },
  "model_tiers": {
    "forbidden": ["high"]
  },
  "settings": {
    "max_review_iterations": 2,
    "max_security_iterations": 1,
    "max_quality_gate_iterations": 2,
    "max_completion_guard_iterations": 2,
    "parallel_execution": false,
    "test_required": true
  }
}
